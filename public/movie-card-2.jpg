<svg width="800" height="600" xmlns="http://www.w3.org/2000/svg">
  <!-- Background -->
  <defs>
    <linearGradient id="bgGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#5D0000; stop-opacity:1"/>
      <stop offset="100%" style="stop-color:#230000; stop-opacity:1"/>
    </linearGradient>
  </defs>
  <rect width="100%" height="100%" fill="url(#bgGradient)"/>
  
  <!-- Chinese temple silhouette -->
  <g opacity="0.2">
    <path d="M100,350 L150,300 L200,330 L250,290 L300,330 L350,280 L400,330 L450,280 L500,330 L550,290 L600,330 L650,300 L700,350 L700,600 L100,600 Z" fill="#000"/>
    
    <!-- Temple roof -->
    <path d="M200,370 Q400,300 600,370 L600,390 Q400,320 200,390 Z" fill="#600"/>
    
    <!-- Pillars -->
    <rect x="230" y="390" width="20" height="210" fill="#500"/>
    <rect x="550" y="390" width="20" height="210" fill="#500"/>
    
    <!-- Steps -->
    <rect x="250" y="550" width="300" height="10" fill="#400"/>
    <rect x="270" y="560" width="260" height="10" fill="#400"/>
    <rect x="290" y="570" width="220" height="10" fill="#400"/>
  </g>
  
  <!-- Yin-Yang Symbol -->
  <g transform="translate(400, 230)">
    <circle cx="0" cy="0" r="100" fill="#fff" opacity="0.2"/>
    <path d="M0,-100 A100,100 0 0,1 0,100 A50,50 0 0,1 0,0 A50,50 0 0,0 0,-100" fill="#000" opacity="0.5"/>
    <circle cx="0" cy="-50" r="15" fill="#000" opacity="0.5"/>
    <circle cx="0" cy="50" r="15" fill="#fff" opacity="0.5"/>
  </g>
  
  <!-- Chinese character for "Kung Fu" -->
  <text x="400" y="430" font-family="Arial, sans-serif" font-size="100" font-weight="bold" text-anchor="middle" fill="#fff" opacity="0.15">功夫</text>
</svg> 