<svg width="800" height="600" xmlns="http://www.w3.org/2000/svg">
  <!-- Background -->
  <defs>
    <linearGradient id="bgGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#332211; stop-opacity:1"/>
      <stop offset="100%" style="stop-color:#000000; stop-opacity:1"/>
    </linearGradient>
  </defs>
  <rect width="100%" height="100%" fill="url(#bgGradient)"/>
  
  <!-- Bamboo Forest Background -->
  <g opacity="0.3">
    <!-- Bamboo 1 -->
    <rect x="100" y="0" width="20" height="600" fill="#425a25" />
    <line x1="100" y1="50" x2="120" y2="50" stroke="#333" stroke-width="2"/>
    <line x1="100" y1="120" x2="120" y2="120" stroke="#333" stroke-width="2"/>
    <line x1="100" y1="190" x2="120" y2="190" stroke="#333" stroke-width="2"/>
    <line x1="100" y1="260" x2="120" y2="260" stroke="#333" stroke-width="2"/>
    <line x1="100" y1="330" x2="120" y2="330" stroke="#333" stroke-width="2"/>
    <line x1="100" y1="400" x2="120" y2="400" stroke="#333" stroke-width="2"/>
    <line x1="100" y1="470" x2="120" y2="470" stroke="#333" stroke-width="2"/>
    <line x1="100" y1="540" x2="120" y2="540" stroke="#333" stroke-width="2"/>
    
    <!-- Bamboo 2 -->
    <rect x="250" y="0" width="15" height="600" fill="#425a25" />
    <line x1="250" y1="70" x2="265" y2="70" stroke="#333" stroke-width="2"/>
    <line x1="250" y1="140" x2="265" y2="140" stroke="#333" stroke-width="2"/>
    <line x1="250" y1="210" x2="265" y2="210" stroke="#333" stroke-width="2"/>
    <line x1="250" y1="280" x2="265" y2="280" stroke="#333" stroke-width="2"/>
    <line x1="250" y1="350" x2="265" y2="350" stroke="#333" stroke-width="2"/>
    <line x1="250" y1="420" x2="265" y2="420" stroke="#333" stroke-width="2"/>
    <line x1="250" y1="490" x2="265" y2="490" stroke="#333" stroke-width="2"/>
    <line x1="250" y1="560" x2="265" y2="560" stroke="#333" stroke-width="2"/>
    
    <!-- Bamboo 3 -->
    <rect x="400" y="0" width="25" height="600" fill="#425a25" />
    <line x1="400" y1="80" x2="425" y2="80" stroke="#333" stroke-width="2"/>
    <line x1="400" y1="160" x2="425" y2="160" stroke="#333" stroke-width="2"/>
    <line x1="400" y1="240" x2="425" y2="240" stroke="#333" stroke-width="2"/>
    <line x1="400" y1="320" x2="425" y2="320" stroke="#333" stroke-width="2"/>
    <line x1="400" y1="400" x2="425" y2="400" stroke="#333" stroke-width="2"/>
    <line x1="400" y1="480" x2="425" y2="480" stroke="#333" stroke-width="2"/>
    <line x1="400" y1="560" x2="425" y2="560" stroke="#333" stroke-width="2"/>
    
    <!-- Bamboo 4 -->
    <rect x="600" y="0" width="18" height="600" fill="#425a25" />
    <line x1="600" y1="60" x2="618" y2="60" stroke="#333" stroke-width="2"/>
    <line x1="600" y1="130" x2="618" y2="130" stroke="#333" stroke-width="2"/>
    <line x1="600" y1="200" x2="618" y2="200" stroke="#333" stroke-width="2"/>
    <line x1="600" y1="270" x2="618" y2="270" stroke="#333" stroke-width="2"/>
    <line x1="600" y1="340" x2="618" y2="340" stroke="#333" stroke-width="2"/>
    <line x1="600" y1="410" x2="618" y2="410" stroke="#333" stroke-width="2"/>
    <line x1="600" y1="480" x2="618" y2="480" stroke="#333" stroke-width="2"/>
    <line x1="600" y1="550" x2="618" y2="550" stroke="#333" stroke-width="2"/>
  </g>
  
  <!-- Crouching Tiger Symbol -->
  <g transform="translate(400, 300)" opacity="0.2">
    <path d="M-100,-30 C-80,-50 -20,-50 0,-30 C20,-50 80,-50 100,-30 C120,-10 120,30 100,50 C80,70 20,70 0,50 C-20,70 -80,70 -100,50 C-120,30 -120,-10 -100,-30 Z" fill="#d4aa00"/>
    <path d="M-50,0 Q-25,-40 0,0 Q25,-40 50,0 Q25,40 0,0 Q-25,40 -50,0 Z" fill="none" stroke="#fff" stroke-width="5"/>
  </g>
  
  <!-- Chinese character for "Hidden Dragon" -->
  <text x="400" y="300" font-family="Arial, sans-serif" font-size="120" font-weight="bold" text-anchor="middle" fill="#fff" opacity="0.15">藏龙</text>
</svg> 